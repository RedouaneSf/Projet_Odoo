<odoo>

    <record id="university_epreuve_form" model="ir.ui.view">
        <field name="name">university.epreuve.form</field>
        <field name="model">university.epreuve</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="lib"/>
                        <field name="date"/>
                        <field name="duree"/>

                    </group>

                </sheet>
            </form>

        </field>
    </record>

    <record id="university_epreuve_tree" model="ir.ui.view">
        <field name="name">university.epreuve.tree</field>
        <field name="model">university.epreuve</field>
        <field name="arch" type="xml">
            <tree>
                        <field name="lib"/>
                        <field name="date"/>
                        <field name="duree"/>

            </tree>

        </field>
    </record>

        <record model="ir.actions.act_window" id="action_university_epreuve">
            <field name="name">list of epreuve</field>
            <field name="res_model">university.epreuve</field>
            <field name="view_mode">tree,form</field>
        </record>

</odoo>

